function validate(){
	var validasi=document.forms["myform"] ["fname"].value;
	if (validasi==null | validasi==""){
		alert("first name harus diisi");
		return false;
	}
	
}
</script>

<form name="myform" action="demo_form.asp" onsubmit="return validate()" method="post">
